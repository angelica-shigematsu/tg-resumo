<%- include('partials/nav.ejs')  %> 
<%- include('partials/header.ejs') %> 

<% volunteers.forEach(volunteer => { %>
  <div class="card w-75">
    <div class="card-body">
      <h5 class="card-title"><%= volunteer.fullName.toUpperCase() %></h5>
      <p class="card-text">
        <p>Cadastrou em:</p>
        <p><%= volunteer.createdAt.getDate() %>/<%= volunteer.createdAt.getMonth()+1 %>/<%= volunteer.createdAt.getFullYear() %></p>
        <a href="/voluntario/<%= volunteer.idVolunteer%>" class="btn btn-primary">Detalhes</a>
    </div>
  </div>
<% }) %>

<%- include('partials/footer.ejs') %> 