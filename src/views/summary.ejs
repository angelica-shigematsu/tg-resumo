<%- include('partials/navSummary.ejs')  %> 
<%- include('partials/header.ejs') %> 

<div class="container-sm">
  <h3>Cadastro de Resumo</h3>
<% if (messageError) {%> 
  <div class="alert alert-warning" role="alert">
    <%=messageError%>
  </div>
<%}%>
  <form id="form-search" enctype="application/x-www-form-urlencoded"
    method="POST" action="/titulo">
    <div class="mb-3 row">
      <div class="col-sm-8">
        <input class="form-control me-2" type="search" placeholder="Procure livros" name="fieldSearch" aria-label="Search"/>
      </div>
      <div class="col-sm-3">
        <button class="btn btn-outline-success col" form="form-search" type="submit">Pesquisar</button>
      </div>
    </div>
  </form>
</div>
<%- include('partials/footer.ejs') %> 
    